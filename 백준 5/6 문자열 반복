#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>

int main() {
	char str[20];
	int n, m;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		scanf("%d %s", &m, str);
		for (int j = 0; j < strlen(str); j++) {
			for (int k = 0; k < m; k++)
				printf("%c", str[j]);
		}
		printf("\n");
	}
}
